<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>sql_connect.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>invalidLogin</servlet-name>
        <servlet-class>sql_connect.UserBean</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetWishDataServlet</servlet-name>
        <servlet-class>sql_connect.GetWishDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnterWishData</servlet-name>
        <servlet-class>sql_connect.EnterWishData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>sql_connect.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ActionServlet</servlet-name>
        <servlet-class>ajaxdemo.ActionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActionServlet</servlet-name>
        <url-pattern>/ActionServlet/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetWishDataServlet</servlet-name>
        <url-pattern>/GetWishDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>sql_connect.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EnterWishData</servlet-name>
        <url-pattern>/EnterWishData</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
